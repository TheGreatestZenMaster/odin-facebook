<% provide(:title, "All Users") %>
<h1>All Users</h1>

<div class="none">
    <ol class="users">
        <% if @users.any? %>
            <% @users.each do |user| %>
                <li><%= render partial: 'users/user', locals: {user: user} %></li>
            <% end %>
            <%= will_paginate @users %>
        <% end %>
    </ol>
</div>


